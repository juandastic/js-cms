{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "project": {
      "model": "projects"
    },
    "category": {
      "model": "categories"
    },
    "author": {
      "model": "authors"
    },
    "excerpt": {
      "type": "string"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
